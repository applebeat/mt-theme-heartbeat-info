$(document).ready(function(){
var $eleW = $("div.movie iframe").attr("width");
var $eleH = $("div.movie iframe").attr("height");

var $aspectRatio = ($eleH / $eleW) * 100 + "%";

$("div.movie").css('padding-top',$aspectRatio);

}
);

